{
  "scripts": {
    "build": "tsc",
    "db:generate": "dotenv -e ../.env -- npx prisma generate",
    "db:migrate:dev": "dotenv -e ../.env -- npx prisma migrate dev",
    "db:studio": "dotenv -e ../.env -- npx prisma studio",
    "dev": "node --watch --experimental-strip-types src/index.ts",
    "prepare": "npm run db:generate",
    "start": "node build/index.js"
  },
  "devDependencies": {
    "@prisma/extension-optimize": "^1.0.3",
    "@types/node": "^22.9.0"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "apollo-server": "^3.13.0",
    "dotenv-cli": "^7.4.3",
    "graphql": "^16.9.0",
    "prisma": "^5.22.0",
    "typescript": "^5.6.3"
  }
}
