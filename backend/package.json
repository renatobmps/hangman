{
  "scripts": {
    "build": "echo 'it is not necessary to build this'",
    "db:generate": "dotenv -e ./.env -- npx prisma generate",
    "db:migrate:dev": "dotenv -e ./.env -- npx prisma migrate dev",
    "db:migrate:dev:create": "dotenv -e ./.env -- npx prisma migrate dev --create-only",
    "db:studio": "dotenv -e ./.env -- npx prisma studio",
    "db:seed": "prisma db seed",
    "dev": "node --watch --experimental-strip-types src/index.ts",
    "prepare": "npm run db:generate",
    "start": "node --no-warnings --watch --experimental-strip-types src/index.ts",
    "test": "node --experimental-strip-types --test --experimental-test-coverage --env-file=.env --no-warnings",
    "test:name": "node --test-name-pattern=$NAME --experimental-strip-types --test --experimental-test-coverage --env-file=.env --no-warnings",
    "test:skip": "node --test-skip-pattern=$NAME --experimental-strip-types --test --experimental-test-coverage --env-file=.env --no-warnings",
    "test:watch": "node --watch --experimental-strip-types --test --env-file=.env --no-warnings"
  },
  "prisma": {
    "seed": "node --experimental-strip-types prisma/seed/index.ts"
  },
  "devDependencies": {
    "@prisma/extension-optimize": "^1.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "apollo-server": "^3.13.0",
    "bcrypt": "^5.1.1",
    "dotenv-cli": "^7.4.3",
    "graphql": "^16.9.0",
    "pg": "^8.13.1",
    "prisma": "^5.22.0",
    "typescript": "^5.6.3"
  },
  "type": "module"
}
