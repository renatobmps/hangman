{
  "scripts": {
    "build": "tsc",
    "db:generate": "dotenv -e ./.env -- npx prisma generate",
    "db:migrate:dev": "dotenv -e ./.env -- npx prisma migrate dev",
    "db:migrate:dev:create": "dotenv -e ./.env -- npx prisma migrate dev --create-only",
    "db:studio": "dotenv -e ./.env -- npx prisma studio",
    "db:migration": "node --watch --experimental-strip-types src/use_cases/database_migration.ts",
    "dev": "node --watch --experimental-strip-types src/index.ts",
    "prepare": "npm run db:generate",
    "start": "node build/index.js",
    "test": "node --experimental-strip-types --test",
    "test:watch": "node --watch --experimental-strip-types --test"
  },
  "devDependencies": {
    "@prisma/extension-optimize": "^1.0.3",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "apollo-server": "^3.13.0",
    "dotenv-cli": "^7.4.3",
    "graphql": "^16.9.0",
    "pg": "^8.13.1",
    "prisma": "^5.22.0",
    "typescript": "^5.6.3"
  },
  "type": "module"
}
